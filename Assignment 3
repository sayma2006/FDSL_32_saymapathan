//Selection sort
#include<stdio.h>
int main()
{
int n=10,a[n],i,j,temp;
printf("Enter the size of array");
scanf("%d",&n);
printf("Enter %d array elements",n);
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
 for(i=0;i<n-1;i++)
  {
    for(j=i+1;j<n;j++)
      { 
         if(a[i]>a[j])
           {
             temp=a[i];
             a[i]=a[j];
             a[j]=temp;
            }
       }
   }
 printf("Sorted array elements are :");
  for(i=0;i<n;i++)
   {
    printf("%d\t",a[i]);
   }
   return 0;
}
 
